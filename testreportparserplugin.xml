  <target name="clean" depends="clean.module.common, clean.module.server, clean.module.agent, clean.module.tests, clean.module.testreportparserplugin" description="cleanup all"/>
  
  <target name="all" depends="init, clean, compile.module.common, compile.module.server, compile.module.agent, compile.module.tests, compile.module.testreportparserplugin" description="build all"/>
</project>